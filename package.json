{
  "name": "@deegital/nuxt-trustup-io-auth",
  "version": "1.0.0",
  "description": "Nuxt module for auth",
  "repository": "git@github.com:deegitalbe/nuxt-trustup-io-auth.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "nuxi prepare playground",
    "dev": "yarn generate && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "build": "yarn validate && nuxt-module-build",
    "lint": "eslint . --fix",
    "ts": "vue-tsc --noEmit",
    "validate": "yarn ts && yarn lint && yarn test",
    "format": "prettier --write \"src/**/*\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "dev:prepare": "nuxt-module-build --stub && yarn generate",
    "preversion": "yarn ts && yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags",
    "prepublishOnly": "yarn ts && yarn lint"
  },
  "dependencies": {
    "@deegital/vue-3-trustup-io-loader": "^0.0.6",
    "@deegital/vue-trustup-io-auth": "^1.0.3",
    "@henrotaymcorp/vue-router-typed-query": "^0.0.1",
    "@nuxt/kit": "^3.6.3",
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@nuxt/devtools": "^0.6.7",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.3",
    "@nuxt/test-utils": "^3.6.3",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^20.4.2",
    "autoprefixer": "^10.4.14",
    "changelogen": "^0.5.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nuxt": "^3.6.3",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.5"
  }
}
